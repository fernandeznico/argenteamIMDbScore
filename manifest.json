{
  "manifest_version": 2,
  "name": "argenteamIMDBscore",
  "version": "1.0",

  "description": "Agrega el puntaje de la película en IMBD a la página princial de argenteam",

  "icons": {
    "48": "IMDb_48.png"
  },

  "content_scripts": [
    {
      "matches": ["http://argenteam.net/"],
      "js": ["argenteamIMDBscore.js"]
    }
  ],

  "permissions": [
      "<all_urls>",
      "*://*.imdb.com/"
  ]
}
